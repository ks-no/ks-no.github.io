        "operationId" : "hentSignerteAvtalerForFiksOrg",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Avtaler",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",

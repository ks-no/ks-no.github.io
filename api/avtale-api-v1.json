          },
          "avtaleUuid" : {
            "type" : "string",
            "format" : "uuid"
          },
          "tjenestekode" : {
            "pattern" : "^[0-9A-Z_.]+$",
            "type" : "string"
          },
          "fiksOrgId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "fiksOrgNavn" : {
            "type" : "string"
          },
          "signeringsdato" : {
            "type" : "string",

          "tjeneste" : {
            "$ref" : "#/components/schemas/TjenesteEnkelDefinisjon"
          },
          "avtaleSignert" : {
            "$ref" : "#/components/schemas/AvtaleSignertDefinisjon"
          }
        }
      },
      "TjenesteEnkelDefinisjon" : {
        "required" : [ "gjeldendeVersjon", "navn", "tjenesteeier", "tjenestekode" ],
        "type" : "object",
        "properties" : {
          "navn" : {
            "pattern" : "[A-Za-zæøåÆØÅ0-9_.,/\\-: ]+",
            "type" : "string"
          },
          "tjenestekode" : {
            "pattern" : "^[0-9A-Z_.]+$",
            "type" : "string"
          },
          "tjenesteeier" : {
            "pattern" : "^[A-Za-zæøåÆØÅ0-9_.,/\\-_ ]+$",
            "type" : "string"
          },
          "beskrivelse" : {
            "type" : "string"
          },
          "gjeldendeVersjon" : {
            "type" : "string"
          },
          "saksnr" : {
            "type" : "string",
            "description" : "Saksnummer i arkivet"
          },
          "lesMer" : {
            "type" : "string",
            "description" : "Lenke til dokumentasjon"
          },
          "ressursType" : {
            "pattern" : "^[0-9A-Z_.]+$",
            "type" : "string"
          }
        }
      },
      "OppdaterAvtaleSignertDefinisjon" : {
        "required" : [ "avtaleUuid", "fiksOrgId", "tjenestekode" ],
        "type" : "object",
        "properties" : {
          "avtaleUuid" : {
            "type" : "string",
            "format" : "uuid"
          },
          "tjenestekode" : {
            "pattern" : "^[0-9A-Z_.]+$",
            "type" : "string"
          },
          "fiksOrgId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "signeringsdato" : {
            "type" : "string",
            "format" : "date"
          }
        }
      },
      "OpprettAvtaleSignertDefinisjon" : {
        "required" : [ "avtaleUuid", "fiksOrgId", "tjenestekode" ],
        "type" : "object",
        "properties" : {
          "avtaleUuid" : {
            "type" : "string",
            "format" : "uuid"
          },
          "tjenestekode" : {
            "pattern" : "^[0-9A-Z_.]+$",
            "type" : "string"
          },
          "fiksOrgId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "signeringsdato" : {
            "type" : "string",
            "format" : "date"
          }
        }
      }
    }

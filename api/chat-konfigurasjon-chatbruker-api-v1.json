{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: Chat Konfigurasjon",
    "description" : "Fiks Chat Konfigurasjon Saksbehandler service",
    "contact" : {
      "name" : "Fiks-brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host"
  }, {
    "url" : "https://api.fiks.test.ks.no",
    "description" : "KS Fiks API test host"
  } ],
  "tags" : [ {
    "name" : "chat-konfigurasjon-chatbruker",
    "description" : "API for konfigurering av Fiks Chat i Forvaltning"
  } ],
  "paths" : {
    "/fiks-chat-konfigurasjon/api/v1/eksternbruker/{fiksOrgId}/sok" : {
      "post" : {
        "tags" : [ "chat-konfigurasjon-chatbruker" ],
        "description" : "Søker etter en saksbehandler i Chat konfigurasjon",
        "operationId" : "hentEksternBruker",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/FinnSaksbehandlerRequest"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SaksbehandlerSokResponse"
                }
              }
            }
          }
        }
      }
    },
    "/fiks-chat-konfigurasjon/api/v1/eksternbruker/opprett" : {
      "post" : {
        "tags" : [ "chat-konfigurasjon-chatbruker" ],
        "description" : "Oppretter en ekstern bruker i RocketChat",
        "operationId" : "opprettEksternBruker",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/OpprettChatbrukerRequest"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : { }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "SaksbehandlerSokResponse" : {
        "required" : [ "etternavn", "fnr", "telefonnummer" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "PersonNummer for saksbehandler"
          },
          "fornavn" : {
            "type" : "string"
          },
          "etternavn" : {
            "type" : "string",
            "description" : "Etternavn for saksbehandler som skal opprettes"
          },
          "telefonnummer" : {
            "pattern" : "^[+]?[0-9]+([- ][0-9]+)*$",
            "type" : "string",
            "description" : "Telefonnummer for saksbehandler som skal opprettes"
          }
        }
      },
      "FinnSaksbehandlerRequest" : {
        "required" : [ "fnr" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "PersonNummer for saksbehandler"
          }
        }
      },
      "OpprettChatbrukerRequest" : {
        "required" : [ "fnr", "kommuneNavn", "saksbehandlerId", "teamId", "telefonnummer" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Personnummer for bruker som skal opprettes"
          },
          "telefonnummer" : {
            "pattern" : "^[+]?[0-9]+([- ][0-9]+)*$",
            "type" : "string",
            "description" : "Telefonnummer for saksbehandler som skal opprettes"
          },
          "teamId" : {
            "type" : "string"
          },
          "kommuneNavn" : {
            "type" : "string"
          },
          "saksbehandlerId" : {
            "type" : "string"
          }
        }
      }
    }
  }
}
{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: eDialog",
    "description" : "Tjeneste for sending av meldinger til organisasjoner via SvarUt",
    "contact" : {
      "name" : "Fiks brukerst√∏tte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  }, {
    "url" : "https://api.test.fiks.ks.no",
    "description" : "KS Fiks TEST API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "Edialog",
    "description" : "API for for sending av meldinger til organisasjoner via SvarUt"
  } ],
  "paths" : {
    "/edialog/api/v1/skjemaer/{id}" : {
      "post" : {
        "tags" : [ "Edialog" ],
        "description" : "Henter et skjema",
        "operationId" : "getSkjema",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Detaljer for skjema",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SkjemaV1"
                }
              }
            }
          }
        }
      }
    },
    "/edialog/api/v1/skjemaer" : {
      "post" : {
        "tags" : [ "Edialog" ],
        "description" : "Oppretter et nytt skjema",
        "operationId" : "nyttSkjema",
        "responses" : {
          "200" : {
            "description" : "Detaljer for nytt skjema",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/NyttSkjemaV1"
                }
              }
            }
          }
        }
      }
    },
    "/edialog/api/v1/skjemaer/{id}/metadata" : {
      "post" : {
        "tags" : [ "Edialog" ],
        "description" : "Oppdaterer metadata for et skjema",
        "operationId" : "oppdaterSkjemaMetadata",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/OppdaterSkjemaMetadataV1"
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Oppdaterte metadata for skjema",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SkjemaMetadataV1"
                }
              }
            }
          }
        }
      }
    },
    "/edialog/api/v1/skjemaer/{id}/send" : {
      "post" : {
        "tags" : [ "Edialog" ],
        "description" : "Sender inn et skjema",
        "operationId" : "sendInnSkjema",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Innsendt skjema",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SkjemaV1"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "SkjemaMetadataV1" : {
        "type" : "object",
        "properties" : {
          "tittel" : {
            "type" : "string"
          },
          "melding" : {
            "type" : "string"
          },
          "sensitivt" : {
            "type" : "boolean"
          }
        }
      },
      "SkjemaV1" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string",
            "format" : "uuid"
          },
          "sistEndret" : {
            "type" : "string",
            "format" : "date-time"
          },
          "innsendtDato" : {
            "type" : "string",
            "format" : "date-time"
          },
          "metadata" : {
            "$ref" : "#/components/schemas/SkjemaMetadataV1"
          }
        }
      },
      "NyttSkjemaV1" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string",
            "format" : "uuid"
          },
          "sistEndret" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "OppdaterSkjemaMetadataV1" : {
        "type" : "object",
        "properties" : {
          "tittel" : {
            "type" : "string"
          },
          "melding" : {
            "type" : "string"
          },
          "sensitivt" : {
            "type" : "boolean"
          }
        }
      }
    }
  }
}
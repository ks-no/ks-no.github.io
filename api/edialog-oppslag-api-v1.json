{
  "openapi": "3.0.1",
  "info": {
    "title": "KS Fiks Melding: Edialog - Oppslag mot Brreg",
    "description": "Edialog Oppslag",
    "contact": {
      "name": "Fiks-brukerstøtte",
      "url": "https://ks-no.github.io/",
      "email": "fiks-utvikling@ks.no"
    },
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://api.fiks.ks.no",
      "description": "KS Fiks API host"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "tags": [
    {
      "name": "Oppslag",
      "description": "API for Edialog - Oppslag mot Brreg"
    }
  ],
  "paths": {
    "/edialog/api/v1/oppslag/{organisasjonsnummer}": {
      "get": {
        "tags": [
          "Oppslag"
        ],
        "description": "Oppslag på organisasjonsnummer mot enhetsregisteret (Brreg)",
        "summary": "Oppslag basert på organsiasjonsnummer organisasjon",
        "operationId": "oppslag",
        "parameters": [
          {
            "name": "organisasjonsnummer",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Organisasjon",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Oppslag"
                }
              }
            }
          },
          "401": {
            "description": "Ingen tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "500": {
            "description": "Systemfeil",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "Oppslag": {
        "type": "object",
        "properties": {
          "organisasjonsnummer": {
            "type": "string",
            "pattern": "^[0-9]{9}$"
          },
          "postadresse": {
            "$ref": "#/components/schemas/Postadresse"
          },
          "status": {
            "type": "string",
            "description": "Status på oppslag",
            "enum": [
              "FUNNET",
              "IKKE_FUNNET",
              "UGYLDIG_IDENTIFIKATOR"
            ]
          }
        },
        "description": "Informasjon om organisasjon hentet fra enhetsregisteret"
      },
      "Postadresse": {
        "type": "object",
        "properties": {
          "navn": {
            "type": "string"
          },
          "adresselinje1": {
            "type": "string"
          },
          "adresselinje2": {
            "type": "string"
          },
          "adresselinje3": {
            "type": "string"
          },
          "postnummer": {
            "type": "string"
          },
          "poststed": {
            "type": "string"
          },
          "land": {
            "type": "string"
          }
        },
        "description": "En postadresse."
      },
      "ErrorMessage": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "error": {
            "type": "string"
          },
          "errorId": {
            "type": "string",
            "format": "uuid"
          },
          "path": {
            "type": "string"
          },
          "originalPath": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "errorCode": {
            "type": "string"
          },
          "errorJson": {
            "type": "string"
          }
        }
      }
    }
  }
}
{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS SvarUt: Hendelser API",
    "description" : "API for henting av hendelser for forsendelse",
    "contact" : {
      "name" : "Fiks-brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v2"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host"
  } ],
  "tags" : [ {
    "name" : "Hendelser",
    "description" : "API for henting av hendelser for forsendelse"
  } ],
  "paths" : {
    "/api/v2/forsendelser/{forsendelseId}/hendelser" : {
      "get" : {
        "tags" : [ "Hendelser" ],
        "description" : "Hent hendelser for forsendelsen",
        "operationId" : "hentHendelser",
        "parameters" : [ {
          "name" : "forsendelseId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Hendelser for forsendelsen",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Hendelser"
                }
              }
            }
          },
          "default" : {
            "description" : "Feil",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/api/v2/forsendelser/{forsendelseId}/signeringshendelser" : {
      "get" : {
        "tags" : [ "Hendelser" ],
        "description" : "Hent signeringshendelser for forsendelsen",
        "operationId" : "hentSigneringshendelser",
        "parameters" : [ {
          "name" : "forsendelseId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Signeringshendelser for forsendelsen",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Signeringshendelser"
                }
              }
            }
          },
          "default" : {
            "description" : "Feil",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Hendelse" : {
        "required" : [ "hendelse", "tidspunkt" ],
        "type" : "object",
        "properties" : {
          "tidspunkt" : {
            "type" : "string",
            "description" : "Tid og dato for hendelsen",
            "format" : "date-time",
            "example" : "2023-01-01T08:13:14.547+01:00"
          },
          "hendelse" : {
            "type" : "string",
            "description" : "Beskrivelse av hendelsen",
            "example" : "Akseptert for ekspedering"
          }
        },
        "description" : "Liste av hendelser"
      },
      "Hendelser" : {
        "required" : [ "hendelser" ],
        "type" : "object",
        "properties" : {
          "hendelser" : {
            "type" : "array",
            "description" : "Liste av hendelser",
            "items" : {
              "$ref" : "#/components/schemas/Hendelse"
            }
          }
        }
      },
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "timestamp" : {
            "type" : "integer",
            "format" : "int64"
          },
          "status" : {
            "type" : "integer",
            "format" : "int32"
          },
          "error" : {
            "type" : "string"
          },
          "errorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "path" : {
            "type" : "string"
          },
          "originalPath" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          },
          "errorCode" : {
            "type" : "string"
          },
          "errorJson" : {
            "type" : "string"
          }
        }
      },
      "Signeringshendelse" : {
        "required" : [ "hendelse", "status", "tidspunkt" ],
        "type" : "object",
        "properties" : {
          "tidspunkt" : {
            "type" : "string",
            "description" : "Tid og dato for hendelsen",
            "format" : "date-time",
            "example" : "2023-01-01T08:13:14.547+01:00"
          },
          "status" : {
            "type" : "string",
            "description" : "Status for signeringen",
            "example" : "VENTER_SIGNERING",
            "enum" : [ "AVVIST_AV_SVARUT", "VENTER_SIGNERING", "SIGNERT_AV_MOTTAKER", "AVVIST_AV_MOTTAKER", "UTLOPT" ]
          },
          "hendelse" : {
            "type" : "string",
            "description" : "Beksrivelse av hendelsen",
            "example" : "Signeringsoppdrag av type AUTENTISERT_SIGNATUR registrert og utløper 07.01.2023"
          }
        },
        "description" : "Liste av signeringshendelser"
      },
      "Signeringshendelser" : {
        "required" : [ "hendelser" ],
        "type" : "object",
        "properties" : {
          "hendelser" : {
            "type" : "array",
            "description" : "Liste av signeringshendelser",
            "items" : {
              "$ref" : "#/components/schemas/Signeringshendelse"
            }
          }
        }
      }
    }
  }
}
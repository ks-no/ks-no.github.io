{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS SvarUt: Lest API",
    "description" : "API for markering av forsendelser som lest",
    "contact" : {
      "name" : "Fiks-brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v2"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "Lest",
    "description" : "API for markering av forsendelser som lest"
  } ],
  "paths" : {
    "/api/v2/forsendelser/{forsendelseId}/lest" : {
      "post" : {
        "tags" : [ "Lest" ],
        "description" : "Markerer forsendelse som lest",
        "operationId" : "settForsendelseLestAvEksterntSystem",
        "parameters" : [ {
          "name" : "forsendelseId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "requestBody" : {
          "content" : {
            "*/*" : {
              "schema" : {
                "$ref" : "#/components/schemas/LestAv"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "204" : {
            "description" : "Forsendelse markert som lest"
          },
          "default" : {
            "description" : "Feil",
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "timestamp" : {
            "type" : "integer",
            "format" : "int64"
          },
          "status" : {
            "type" : "integer",
            "format" : "int32"
          },
          "error" : {
            "type" : "string"
          },
          "errorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "path" : {
            "type" : "string"
          },
          "originalPath" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          },
          "errorCode" : {
            "type" : "string"
          },
          "errorJson" : {
            "type" : "string"
          }
        }
      },
      "LestAv" : {
        "required" : [ "fnr", "tidspunkt" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Fødselsnummer for personen som leste forsendelsen",
            "example" : "12345678910"
          },
          "tidspunkt" : {
            "type" : "string",
            "description" : "Tidspunkt for når forsendelsen ble lest",
            "format" : "date-time",
            "example" : "2023-01-01T12:44:06.123+01:00"
          },
          "eksterntSystemnavn" : {
            "type" : "string",
            "description" : "Navn på systemet hvor forsendelsen ble lest"
          }
        }
      }
    }
  }
}
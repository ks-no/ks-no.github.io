{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS SvarUt: Slett API",
    "description" : "API for sletting av dokumenter for forsendelser",
    "contact" : {
      "name" : "Fiks-brukerst√∏tte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v2"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host"
  } ],
  "tags" : [ {
    "name" : "Slett",
    "description" : "API for sletting av dokumenter for forsendelser"
  } ],
  "paths" : {
    "/api/v2/forsendelser/{forsendelseId}/dokumenter" : {
      "delete" : {
        "tags" : [ "Slett" ],
        "description" : "Sletter dokumenter knyttet til forsendelse",
        "operationId" : "slettFiler",
        "parameters" : [ {
          "name" : "forsendelseId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        }, {
          "name" : "slettetype",
          "in" : "query",
          "schema" : {
            "type" : "string",
            "description" : "Definerer hvilke dokumenter som skal slettes",
            "default" : "ORIGINALDOKUMENTER",
            "enum" : [ "ORIGINALDOKUMENTER", "ALLE" ]
          }
        } ],
        "responses" : {
          "204" : {
            "description" : "Dokumenter slettet"
          },
          "default" : {
            "description" : "Feil",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "timestamp" : {
            "type" : "integer",
            "format" : "int64"
          },
          "status" : {
            "type" : "integer",
            "format" : "int32"
          },
          "error" : {
            "type" : "string"
          },
          "errorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "path" : {
            "type" : "string"
          },
          "originalPath" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          },
          "errorCode" : {
            "type" : "string"
          },
          "errorJson" : {
            "type" : "string"
          }
        }
      }
    }
  }
}
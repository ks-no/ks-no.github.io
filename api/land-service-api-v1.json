{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Fiks Land",
    "description" : "Register over land med landkoder, som alpha 3 kode",
    "contact" : {
      "name" : "Fiks-brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host"
  } ],
  "tags" : [ {
    "name" : "Land",
    "description" : "API for oppslag på land med navn for ulike språk"
  } ],
  "paths" : {
    "/land/api/v1/{sprakkode}" : {
      "get" : {
        "tags" : [ "Land" ],
        "description" : "Oppslag på landkoder",
        "operationId" : "hentLandkoder",
        "parameters" : [ {
          "name" : "sprakkode",
          "in" : "path",
          "required" : true,
          "schema" : {
            "$ref" : "#/components/schemas/Sprakkode"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Henter liste over landkoder med navn basert på språk",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/LandkodeResult"
                }
              }
            }
          },
          "404" : {
            "description" : "Ugyldig språkkode",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "500" : {
            "description" : "Systemfeil",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Landkode" : {
        "type" : "object",
        "properties" : {
          "kode" : {
            "type" : "string"
          },
          "navn" : {
            "type" : "string"
          }
        }
      },
      "LandkodeResult" : {
        "type" : "object",
        "properties" : {
          "landkoder" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Landkode"
            }
          }
        }
      },
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "source" : {
            "type" : "object"
          },
          "message" : {
            "type" : "string"
          },
          "severity" : {
            "type" : "string",
            "enum" : [ "FATAL", "WARNING", "HINT" ]
          }
        }
      },
      "Sprakkode" : {
        "type" : "string",
        "enum" : [ "Bokmal", "Nynorsk", "Engelsk" ]
      }
    }
  }
}
          },
          "antallLedsagere" : {
            "type" : "integer",
            "description" : "Antall ledsagere",
            "format" : "int32"
          },
          "utlopsdato" : {
            "type" : "string",
            "description" : "Utløpsdato for beviset",
            "format" : "date"
          },
          "administratorer" : {
            "uniqueItems" : true,
            "type" : "array",
            "description" : "Personer som skal ha administratorrett for et ledsagerbeviset",
            "items" : {
              "$ref" : "#/components/schemas/PersonRespons"
            }
          },
          "eksternId" : {
            "type" : "string",
            "description" : "Ekstern id for ledsagerbevis"
          },
          "fiksOrgId" : {
            "type" : "string",
            "description" : "Organisasjon som har utstedet bevis",
            "format" : "uuid"
          },
          "opprettetDato" : {
            "type" : "string",
            "description" : "Dato ledsagerbeviset er opprettet",
            "format" : "date-time"
          },
          "revokertGrunn" : {
            "type" : "string",
            "description" : "Hvorfor ledsagerbeviset er slettet/revokert"
          },
          "revokertDato" : {
            "type" : "string",
            "description" : "Dato ledsagerbeviset er revokert",
            "format" : "date-time"
          }
        }
      },
      "PersonRespons" : {
        "required" : [ "fnr", "navn", "telefonnummer" ],
        "type" : "object",
        "properties" : {
          "navn" : {
            "type" : "string",
            "description" : "Navn på administrator"
          },
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Fødselsnummer til administrator"
          },
          "telefonnummer" : {
            "type" : "string",
            "description" : "Telefonnummer til administrator"
          }
        },
        "description" : "Administrator av ledsagerbevis"
      },
      "FjernAdministratorRequest" : {
        "required" : [ "fnr" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Fødselsnummer til person"
          }
        },
        "description" : "Person som skal fjernes som administrator av ledsagerbevis"
      },
      "LedsagerbevisHendelse" : {
        "required" : [ "hendelseType", "tidspunkt" ],
        "type" : "object",
        "properties" : {
          "hendelseType" : {
            "type" : "string",
            "description" : "Type hendelse på ledsagerbeviset",

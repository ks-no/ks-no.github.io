          },
          "tidspunkt" : {
            "type" : "string",
            "description" : "Tidspunkt hendelsen inntraff",
            "format" : "date"
          }
        },
        "description" : "Hendelse på ledsagerbevis"
      },
      "LedsagerbevisTilgang" : {
        "required" : [ "fiksOrgId", "fiksOrgNavn" ],
        "type" : "object",
        "properties" : {
          "fiksOrgId" : {

{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: Ledsagerbevis Konfigurasjon",
    "description" : "Konfigurasjon av tjenesten digitalt ledsagerbevis",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://forvaltning.fiks.ks.no",
    "description" : "KS Fiks API host"
  }, {
    "url" : "https://forvaltning.test.fiks.ks.no",
    "description" : "KS Fiks TEST API host"
  } ],
  "tags" : [ {
    "name" : "Ledsagerbevis Konfigurasjon",
    "description" : "API for konfigurasjon av tjenesten digitalt ledsagerbevis"
  } ],
  "paths" : {
    "/digitalt-ledsagerbevis-konfigurasjon/api/v1/organisasjoner/{fiksOrgId}" : {
      "get" : {
        "tags" : [ "Ledsagerbevis Konfigurasjon" ],
        "summary" : "Henter status på om organisasjon har aktivert dokumentlagerkonto og registerRolle",
        "operationId" : "hentStatus",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Status på om organisasjon har opprettet Dokumentlager-konto",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/StatusRetur"
                }
              }
            }
          }
        }
      },
      "post" : {
        "tags" : [ "Ledsagerbevis Konfigurasjon" ],
        "summary" : "Oppretter ny Dokumentlager-konto og registerrolle som brukes til utsendelse av informasjon til de som skal få digitalt ledsagerbevis",
        "operationId" : "opprettLedsagerbevisKonfigurasjon",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Dokumentlager-konto og register-rolle opprettet",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/LedsagerBevisKonfigurasjon"
                }
              }
            }
          }
        }
      }
    },
    "/digitalt-ledsagerbevis-konfigurasjon/api/v1/organisasjoner/{fiksOrgId}/svarut/{svarutkontoid}" : {
      "post" : {
        "tags" : [ "Ledsagerbevis Konfigurasjon" ],
        "summary" : "Spesifiser hvilken Svarut-konto som skal brukes under utsendelse av brev",
        "operationId" : "leggTilSvarutkonto",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        }, {
          "name" : "svarutkontoid",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Svarut-kontoId er lagret"
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "StatusRetur" : {
        "required" : [ "dokumentlagerKontoStatus", "rolleStatus", "svarutKontoStatus" ],
        "type" : "object",
        "properties" : {
          "dokumentlagerKontoStatus" : {
            "type" : "string",
            "enum" : [ "OPPRETTET", "IKKE_OPPRETTET" ]
          },
          "rolleStatus" : {
            "type" : "string",
            "enum" : [ "OPPRETTET", "IKKE_OPPRETTET" ]
          },
          "svarutKontoStatus" : {
            "type" : "string",
            "enum" : [ "OPPRETTET", "IKKE_OPPRETTET" ]
          }
        },
        "description" : "Informasjon om status for konfigurasjon av digitalt ledsagerbevis"
      },
      "LedsagerBevisKonfigurasjon" : {
        "required" : [ "dokumentlagerKontoId", "rolleId" ],
        "type" : "object",
        "properties" : {
          "dokumentlagerKontoId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "rolleId" : {
            "type" : "string",
            "format" : "uuid"
          }
        }
      }
    }
  }
}
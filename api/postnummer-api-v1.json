{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: Postnummer",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host"
  }, {
    "url" : "https://api.test.fiks.ks.no",
    "description" : "KS Fiks TEST API host"
  } ],
  "tags" : [ {
    "name" : "Postnummer",
    "description" : "API for å hente postnummer importert via tekstfil fra bring.no (https://www.bring.no/postnummerregister-ansi.txt)"
  } ],
  "paths" : {
    "/postnummer/api/v1/{postnummer}" : {
      "get" : {
        "tags" : [ "Postnummer" ],
        "description" : "Henter informasjon basert på postnummer.",
        "operationId" : "finnFraPostnummer",
        "parameters" : [ {
          "name" : "postnummer",
          "in" : "path",
          "required" : true,
          "schema" : {
            "pattern" : "^[0-9]{4}$",
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "OK",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Postnummer"
                }
              }
            }
          },
          "400" : {
            "description" : "Bad request",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "401" : {
            "description" : "Ikke autentisert",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "500" : {
            "description" : "Systemfeil",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/postnummer/api/v1/{postnummer}/poststed" : {
      "get" : {
        "tags" : [ "Postnummer" ],
        "description" : "Henter poststed basert på postnummer.",
        "operationId" : "finnPoststed",
        "parameters" : [ {
          "name" : "postnummer",
          "in" : "path",
          "required" : true,
          "schema" : {
            "pattern" : "^[0-9]{4}$",
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "OK",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Poststed"
                }
              }
            }
          },
          "400" : {
            "description" : "Bad request",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "401" : {
            "description" : "Ikke autentisert",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "500" : {
            "description" : "Systemfeil",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Postnummer" : {
        "type" : "object",
        "properties" : {
          "postnummer" : {
            "type" : "string",
            "description" : "Postnummer"
          },
          "poststed" : {
            "type" : "string",
            "description" : "Poststed"
          },
          "kommunenummer" : {
            "type" : "string",
            "description" : "Kommunenummer"
          },
          "kommune" : {
            "type" : "string",
            "description" : "Kommune"
          },
          "postnummerKategori" : {
            "type" : "string",
            "description" : "PostnummerKategori: <br> B - Både gateadresser og postbokser <br> F - Flere bruksområder (felles) <br> G - Gateadresser (og stedsadresser), dvs. “grønne postkasser”<br> P - Postbokser<br> S - Servicepostnummer (disse postnumrene er ikke i bruk til postadresser)",
            "enum" : [ "B", "F", "G", "P", "S" ]
          }
        }
      },
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "timestamp" : {
            "type" : "integer",
            "format" : "int64"
          },
          "status" : {
            "type" : "integer",
            "format" : "int32"
          },
          "error" : {
            "type" : "string"
          },
          "errorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "path" : {
            "type" : "string"
          },
          "originalPath" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          },
          "errorCode" : {
            "type" : "string"
          },
          "errorJson" : {
            "type" : "string"
          }
        }
      },
      "Poststed" : {
        "type" : "object",
        "properties" : {
          "poststed" : {
            "type" : "string",
            "description" : "Poststed"
          }
        }
      }
    }
  }
}
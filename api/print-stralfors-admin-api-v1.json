{
  "openapi": "3.0.1",
  "info": {
    "title": "KS Fiks: Print Stralfors",
    "description": "Service for  admin av print",
    "contact": {
      "name": "Fiks-brukerstøtte",
      "url": "https://ks-no.github.io/",
      "email": "fiks-utvikling@ks.no"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://forvaltning.fiks.ks.no",
      "description": "KS Fiks API host"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "tags": [
    {
      "name": "PrintStralforsAdmin",
      "description": "API for admin ting av print til strålfors"
    },
    {
      "name": "Csv",
      "description": "Api for å hente ned CSV filer"
    }
  ],
  "paths": {
    "/stralfors/api/v1/admin/advarsel": {
      "get": {
        "tags": [
          "Csv"
        ],
        "summary": "Henter brev som ikke er printet innenfor fristen i måned",
        "operationId": "advarsel",
        "parameters": [
          {
            "name": "year",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "month",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "CSV med filer som ikke er printet innenfor fristen i måned",
            "content": {
              "text/csv": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    },
    "/stralfors/api/v1/admin/manuell-handtering": {
      "post": {
        "tags": [
          "PrintStralforsAdmin"
        ],
        "summary": "Marker brev som skal håndteres manuelt",
        "operationId": "manuellHandtering",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ManuellHandteringRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "ManuellHandteringRequest": {
        "type": "object",
        "required": [
          "forsendelseIdListe"
        ],
        "properties": {
          "forsendelseIdListe": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uuid"
            },
            "minItems": 1,
            "maxItems": 1000
          },
          "begrunnelse": {
            "type": "string",
            "maxLength": 1000
          }
        }
      }
    }
  }
}
{
  "openapi": "3.0.1",
  "info": {
    "title": "KS Fiks: Print Stralfors",
    "description": "Service for  admin av print",
    "contact": {
      "name": "Fiks-brukerstøtte",
      "url": "https://ks-no.github.io/",
      "email": "fiks-utvikling@ks.no"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://forvaltning.fiks.ks.no",
      "description": "KS Fiks API host"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "tags": [
    {
      "name": "PrintStralforsAdmin",
      "description": "API for admin ting av print til strålfors"
    },
    {
      "name": "Csv",
      "description": "Api for å hente ned CSV filer"
    }
  ],
  "paths": {
    "/stralfors/api/v1/admin/printjobber": {
      "get": {
        "tags": [
          "Csv"
        ],
        "summary": "Henter CSV med liste over printjobber",
        "operationId": "hentPrintjobber",
        "parameters": [
          {
            "name": "fraDato",
            "description": "Startdato for å filtrere forsendelser (inkludert)",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "tilDato",
            "description": "Sluttdato for å filtrere forsendelser (inkludert)",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "eldreEnnTimer",
            "description": "Parameter for å hente forsendelser som er eldre enn X timer siden det ble sendt til print, innen virkedager",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "minimum": 1,
              "maximum": 100
            }
          },
          {
            "name": "erPrintet",
            "description": "Parameter for å hente forsendelser som er printet eller ei",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "tjeneste",
            "description": "Parameter for å begrense henting av forsendelser til oppgitt tjeneste",
            "in": "query",
            "required": false,
            "schema": {
              "$ref": "#/components/schemas/Tjeneste"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "CSV som inneholder forsendelseId, dato for når brevet ble sendt til print og dato for når brevet ble printet",
            "content": {
              "text/csv": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "default": {
            "description": "Noe gikk galt",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/stralfors/api/v1/admin/printjobber/prikkbelastning": {
      "get": {
        "tags": [
          "Csv"
        ],
        "summary": "Henter CSV med liste over printjobber",
        "operationId": "hentPrintjobberMedPrikkbelastning",
        "parameters": [
          {
            "name": "year",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            },
            "description": "År for å filtrere listen av forsendelser"
          },
          {
            "name": "month",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32",
              "minimum": 1,
              "maximum": 12
            },
            "description": "Måned for å filtrere listen av forsendelser. Dersom måned er satt, må også år være satt. 1 = januar, 12 = desember"
          }
        ],
        "responses": {
          "200": {
            "description": "CSV som inneholder forsendelseId, dato for når brevet ble sendt til print og dato for når brevet ble printet",
            "content": {
              "text/csv": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "default": {
            "description": "Noe gikk galt",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/stralfors/api/v1/admin/manuell-handtering": {
      "post": {
        "tags": [
          "PrintStralforsAdmin"
        ],
        "summary": "Marker brev som skal håndteres manuelt",
        "operationId": "markerManueltHandtert",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ManuellHandteringRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          },
          "default": {
            "description": "Noe gikk galt",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "ManuellHandteringRequest": {
        "type": "object",
        "required": [
          "forsendelseIdListe"
        ],
        "properties": {
          "forsendelseIdListe": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uuid"
            },
            "minItems": 1,
            "maxItems": 1000
          },
          "begrunnelse": {
            "type": "string",
            "maxLength": 1000
          }
        }
      },
      "ErrorMessage": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "error": {
            "type": "string"
          },
          "errorId": {
            "type": "string",
            "format": "uuid"
          },
          "path": {
            "type": "string"
          },
          "originalPath": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "errorCode": {
            "type": "string"
          },
          "errorJson": {
            "type": "string"
          }
        }
      },
      "Tjeneste": {
        "type": "string",
        "description": "Tjeneste som benytter Strålfors",
        "enum": [
          "BEKYMRINGSMELDING",
          "SVARUT",
          "DIGITALT_LEDSAGERBEVIS"
        ]
      }
    }
  }
}
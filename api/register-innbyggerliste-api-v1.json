{
  "openapi": "3.0.1",
  "info": {
    "title": "KS Fiks register innbyggerliste service",
    "description": "Folkregister innbyggerliste service",
    "contact": {
      "name": "Fiks brukerstøtte",
      "url": "https://www.ks.no/fagomrader/digitalisering/utviklingsprosjekter",
      "email": "fiks-utvikling@ks.no"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://api.fiks.ks.no",
      "description": "KS Fiks API host",
      "variables": {}
    },
    {
      "url": "https://api.fiks.test.ks.no",
      "description": "KS Fiks TEST API host",
      "variables": {}
    }
  ],
  "tags": [
    {
      "name": "innbyggerliste",
      "description": "API for å håndtere funksjonalitet rundt innbyggerlister"
    },
    {
      "name": "innbyggerliste-hendelser",
      "description": "API for å følge opp status på eksisterende innbyggerlister"
    }
  ],
  "paths": {
    "/register/api/v1/ks/innbyggerliste/v1/verdier": {
      "get": {
        "tags": [
          "innbyggerliste"
        ],
        "description": "Gyldige verdier for nøkkelfelt, til bruk i front-end",
        "operationId": "gyldigeVerdier",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GyldigeVerdier"
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/register/api/v1/ks/innbyggerliste/v1/{fiksOrgId}": {
      "get": {
        "tags": [
          "innbyggerliste"
        ],
        "description": "Henter innbyggerlister man har tilgang tilhørende gitt organisasjon",
        "operationId": "listInnbyggerlister",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "status",
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "OPPRETTET",
                  "HOS_SKATT",
                  "FERDIG_SKATT",
                  "FERDIG",
                  "FEILET"
                ]
              }
            }
          },
          {
            "name": "rolleId",
            "in": "query",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisterResponse"
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "innbyggerliste"
        ],
        "description": "Oppretter innbyggerliste for en organisasjon",
        "operationId": "opprett",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OpprettInnbyggerlisteFraUttrekk"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Opprettet",
            "links": {
              "_self": {
                "operationRef": "status",
                "parameters": {
                  "fiksOrgId": "$request.path.fiksOrgId",
                  "innbyggerlisteId": "$response.body#/ref"
                }
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisteRef"
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/register/api/v1/ks/innbyggerliste/v1/{fiksOrgId}/soek": {
      "post": {
        "tags": [
          "innbyggerliste"
        ],
        "description": "Oppretter innbyggerliste for en organisasjon basert på et personsøk",
        "operationId": "opprettFraSoek",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OpprettInnbyggerlisteFraSoek"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisteFerdig"
                }
              }
            }
          },
          "201": {
            "description": "Opprettet",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisteRef"
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/register/api/v1/ks/innbyggerliste/v1/{fiksOrgId}/soek/personnummer/{rolleId}/csv": {
      "post": {
        "tags": [
          "innbyggerliste"
        ],
        "description": "Henter innbyggerliste basert på en csv-fil med personidentifikatorer",
        "operationId": "soekMedPersonidentifikatorCsv",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "rolleId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "content": {
            "text/csv": {
              "schema": {
                "type": "string",
                "format": "binary"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/csv": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    },
    "/register/api/v1/ks/innbyggerliste/v1/{fiksOrgId}/soek/adresse/{rolleId}": {
      "post": {
        "tags": [
          "innbyggerliste"
        ],
        "description": "Henter innbyggerliste basert på en csv-fil med adresser og husnummer",
        "operationId": "soekMedAdresser",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "rolleId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "text/csv": {
              "schema": {
                "type": "string",
                "format": "binary"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Søk opprettet",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisteRef"
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/register/api/v1/ks/innbyggerliste/v1/{fiksOrgId}/{innbyggerlisteId}": {
      "get": {
        "tags": [
          "innbyggerliste"
        ],
        "description": "Henter status for innbyggerliste",
        "operationId": "status",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "innbyggerlisteId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisteStatusResponse"
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "404": {
            "description": "Det finnes ikke noe utrekk med den gitte innbyggerlisteId-en",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "innbyggerliste"
        ],
        "operationId": "slettInnbyggerliste",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "innbyggerlisteId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "204": {
            "description": "No Content"
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/register/api/v1/ks/innbyggerliste/v1/events/{fiksOrgId}": {
      "get": {
        "tags": [
          "innbyggerliste-hendelser"
        ],
        "description": "Brukes til å lese alle hendelser for alle innbyggerlister man har tilgang til",
        "operationId": "lesAlleEventer",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "200": {
            "description": "Ok",
            "content": {
              "text/event-stream": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisteHendelse"
                }
              }
            }
          }
        }
      }
    },
    "/register/api/v1/ks/innbyggerliste/v1/events/{fiksOrgId}/{rolleId}": {
      "get": {
        "tags": [
          "innbyggerliste-hendelser"
        ],
        "description": "Brukes til å lese alle hendelser for alle innbyggerlister som gjøres for en gitt rolle",
        "operationId": "lesAlleEventerForRolle",
        "parameters": [
          {
            "name": "fiksOrgId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "rolleId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "400": {
            "description": "Feil input",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "200": {
            "description": "Ok",
            "content": {
              "text/event-stream": {
                "schema": {
                  "$ref": "#/components/schemas/InnbyggerlisteHendelse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GyldigeVerdier": {
        "required": [
          "innbyggerStatus",
          "kjoenn",
          "landStatsborgerskap",
          "sivilstand",
          "status",
          "tilhorighetKommune"
        ],
        "type": "object",
        "properties": {
          "status": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Verdi"
            }
          },
          "innbyggerStatus": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Verdi"
            }
          },
          "sivilstand": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Verdi"
            }
          },
          "kjoenn": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Verdi"
            }
          },
          "tilhorighetKommune": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Verdi"
            }
          },
          "landStatsborgerskap": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Statsborgerskap"
            }
          }
        }
      },
      "Statsborgerskap": {
        "required": [
          "kode",
          "landKilde",
          "navn"
        ],
        "type": "object",
        "properties": {
          "kode": {
            "type": "string"
          },
          "navn": {
            "type": "string"
          },
          "landKilde": {
            "type": "string",
            "enum": [
              "ISO",
              "UDI"
            ]
          }
        }
      },
      "Verdi": {
        "type": "object",
        "properties": {
          "kode": {
            "type": "string"
          },
          "visningsnavn": {
            "type": "string"
          },
          "beskrivelse": {
            "type": "string"
          }
        }
      },
      "AarIntervall": {
        "type": "object",
        "properties": {
          "fraAar": {
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "tilAar": {
            "type": "integer",
            "format": "int32"
          }
        },
        "description": "Fødselsår, fra og med, til og med"
      },
      "EksportFormatDefinisjon": {
        "type": "string",
        "enum": [
          "EGENDEFINERT",
          "VAKSINE"
        ]
      },
      "FerdigInnbyggerliste": {
        "title": "FerdigInnbyggerliste",
        "required": [
          "dokumentlagerId",
          "id",
          "innbyggereHentet",
          "kilde",
          "opprettet",
          "rolleId",
          "sistOppdatert"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "innbyggereHentet": {
            "type": "integer",
            "format": "int64"
          },
          "sistOppdatert": {
            "type": "string",
            "format": "date-time"
          },
          "dokumentlagerId": {
            "type": "string",
            "format": "uuid"
          },
          "beskrivelse": {
            "type": "string"
          },
          "opprettet": {
            "type": "string",
            "format": "date-time"
          },
          "rolleId": {
            "type": "string",
            "format": "uuid"
          },
          "kilde": {
            "$ref": "#/components/schemas/KildeSystem"
          },
          "utlopsdato": {
            "type": "string",
            "format": "date-time"
          }
        },
        "readOnly": true,
        "discriminator": {
          "propertyName": "kilde",
          "mapping": {
            "innbyggerliste": "#/components/schemas/FerdigInnbyggerlisteFraUttrekk",
            "SOEK": "#/components/schemas/FerdigInnbyggerlisteFraSoek"
          }
        }
      },
      "FerdigInnbyggerlisteFraSoek": {
        "required": [
          "definisjon",
          "dokumentlagerId",
          "id",
          "innbyggereHentet",
          "kilde",
          "opprettet",
          "rolleId",
          "sistOppdatert"
        ],
        "type": "object",
        "readOnly": true,
        "allOf": [
          {
            "$ref": "#/components/schemas/FerdigInnbyggerliste"
          },
          {
            "type": "object",
            "properties": {
              "definisjon": {
                "$ref": "#/components/schemas/PersonSoekDefinisjon"
              }
            }
          }
        ]
      },
      "FerdigInnbyggerlisteFraUttrekk": {
        "required": [
          "definisjon",
          "dokumentlagerId",
          "id",
          "innbyggereHentet",
          "kilde",
          "opprettet",
          "rolleId",
          "sistOppdatert"
        ],
        "type": "object",
        "readOnly": true,
        "allOf": [
          {
            "$ref": "#/components/schemas/FerdigInnbyggerliste"
          },
          {
            "type": "object",
            "properties": {
              "definisjon": {
                "$ref": "#/components/schemas/UttrekkDefinisjon"
              }
            }
          }
        ]
      },
      "InnbyggerlisteFraSoekResponse": {
        "required": [
          "definisjon",
          "fiksOrg",
          "id",
          "innbyggereHentet",
          "innbyggerlisteStatus",
          "kilde",
          "opprettet",
          "rolleId",
          "sistOppdatert"
        ],
        "type": "object",
        "readOnly": true,
        "allOf": [
          {
            "$ref": "#/components/schemas/InnbyggerlisteStatusResponse"
          },
          {
            "type": "object",
            "properties": {
              "definisjon": {
                "$ref": "#/components/schemas/PersonSoekDefinisjon"
              }
            }
          }
        ]
      },
      "InnbyggerlisteFraUttrekkResponse": {
        "required": [
          "definisjon",
          "fiksOrg",
          "id",
          "innbyggereHentet",
          "innbyggerlisteStatus",
          "kilde",
          "opprettet",
          "rolleId",
          "sistOppdatert"
        ],
        "type": "object",
        "readOnly": true,
        "allOf": [
          {
            "$ref": "#/components/schemas/InnbyggerlisteStatusResponse"
          },
          {
            "type": "object",
            "properties": {
              "definisjon": {
                "$ref": "#/components/schemas/UttrekkDefinisjon"
              }
            }
          }
        ]
      },
      "InnbyggerlisteStatus": {
        "type": "string",
        "enum": [
          "OPPRETTET",
          "HOS_SKATT",
          "FERDIG_SKATT",
          "FERDIG",
          "FEILET"
        ]
      },
      "InnbyggerlisteStatusResponse": {
        "required": [
          "fiksOrg",
          "id",
          "innbyggereHentet",
          "innbyggerlisteStatus",
          "kilde",
          "opprettet",
          "rolleId",
          "sistOppdatert"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "innbyggerlisteStatus": {
            "$ref": "#/components/schemas/InnbyggerlisteStatus"
          },
          "innbyggereHentet": {
            "type": "integer",
            "format": "int64"
          },
          "sistOppdatert": {
            "type": "string",
            "format": "date-time"
          },
          "beskrivelse": {
            "type": "string"
          },
          "opprettet": {
            "type": "string",
            "format": "date-time"
          },
          "rolleId": {
            "type": "string",
            "format": "uuid"
          },
          "fiksOrg": {
            "type": "string",
            "format": "uuid"
          },
          "kilde": {
            "$ref": "#/components/schemas/KildeSystem"
          },
          "utlopsdato": {
            "type": "string",
            "format": "date-time"
          }
        },
        "readOnly": true,
        "discriminator": {
          "propertyName": "kilde",
          "mapping": {
            "innbyggerliste": "#/components/schemas/InnbyggerlisteFraUttrekkResponse",
            "SOEK": "#/components/schemas/InnbyggerlisteFraSoekResponse"
          }
        }
      },
      "InnbyggerlisterResponse": {
        "required": [
          "feilede",
          "ferdige"
        ],
        "type": "object",
        "properties": {
          "ferdige": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FerdigInnbyggerliste"
            },
            "nullable": false
          },
          "feilede": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/InnbyggerlisteStatusResponse"
            },
            "nullable": false
          },
          "igang": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/InnbyggerlisteStatusResponse"
            }
          }
        }
      },
      "KildeSystem": {

  "components" : {
    "schemas" : {
      "PersonnavnDefinisjon" : {
        "required" : [ "etternavn", "fornavn" ],
        "type" : "object",
        "properties" : {
          "etternavn" : {
            "type" : "string"
          },
          "fornavn" : {
            "type" : "string"
          },
          "mellomnavn" : {
            "type" : "string"
          }
        }
      },
      "StadieDefinisjon" : {
        "type" : "string",
        "enum" : [ "OPPGJOER", "UTKAST", "UKJENT" ]
      },
      "SummertSkattegrunnlagDefinisjonV2" : {
        "required" : [ "inntektsaar", "stadie", "summertSkattegrunnlagPersoner", "samletBeloep", "samletGrunnlag", "soeketidspunkt" ],
        "type" : "object",
        "properties" : {
          "inntektsaar" : {
            "type" : "integer",
            "format" : "int32"
          },
          "stadie" : {
            "$ref" : "#/components/schemas/StadieDefinisjon"
          },
          "summertSkattegrunnlagPersoner" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/SummertSkattegrunnlagPersonDefinisjonV2"
            }
          },
          "samletBeloep" : {
            "type" : "integer",
            "format" : "int64"
          },
          "samletGrunnlag" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/GruppeDefinisjon"
            }
          },
          "soeketidspunkt" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "SummertSkattegrunnlagPersonDefinisjonV2" : {
        "required" : [ "personidentifikator", "skjermet", "stadie", "beloep", "grunnlag" ],
        "type" : "object",
        "properties" : {
          "personidentifikator" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string"
          },
          "personnavn" : {
            "$ref" : "#/components/schemas/PersonnavnDefinisjon"
          },
          "skjermet" : {
            "type" : "boolean"
          },
          "skatteoppgjoersdato" : {
            "type" : "string",
            "format" : "date"
          },
          "stadie" : {
            "$ref" : "#/components/schemas/StadieDefinisjon"
          },
          "registreringstidpunkt" : {
            "type" : "string",
            "format" : "date-time"
          },
          "beloep" : {
            "type" : "integer",
            "format" : "int64"
          },
          "grunnlag" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/GruppeDefinisjon"
            }
          },
          "skattFeilmelding" : {
            "type" : "string"
          }
        }
      },
      "SummertSkattegrunnlagVerdierDefinisjon": {
        "type" : "object",
        "properties" : {
          "navVerdier" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/EkstraPostVerdiDefinisjon"
            }
          },
          "kommuneVerdier" : {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/EkstraPostVerdiDefinisjon"
            }
          }
        }
      },
      "GruppeDefinisjon": {
        "required" : [ "navn", "beskrivelse", "beloep", "operasjon" ],
        "type" : "object",
        "properties" : {
          "navn" : {
            "type" : "string"
          },
          "beskrivelse" : {
            "type" : "string"
          },
          "beloep" : {
            "type" : "integer",
            "format" : "int64"
          },
          "operasjon" : {
            "$ref" : "#/components/schemas/OperasjonDefinisjon"
          },
          "poster" : {
            "type" : "array",
            "items": {
              "$ref" : "#/components/schemas/PostDefinisjon"
            }
          }
        }
      },
      "SummertSkattegrunnlagSoekDefinisjonV2": {
        "required" : [ "soekere", "inntektsaar", "beregningstype" ],
        "type" : "object",
        "properties" : {
          "soekere" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/HusstandsmedlemDefinisjon"
            }
          },
          "inntektsaar" : {
            "minimum" : 2021,
            "maximum" : 2022,
            "type" : "integer",
            "format" : "int32"
          },
          "beregningstype" : {
            "$ref" : "#/components/schemas/BeregningstypeDefinisjon"
          }
        }
      },
      "HusstandsmedlemDefinisjon": {
        "required" : [ "personidentifikator", "personType" ],
        "type" : "object",
        "properties" : {
          "personidentifikator": {
            "description" : "Fødsels- eller D-nummer.",
            "type" : "string",
            "pattern": "^[0-9]{11}$"
          },
          "personType": {
            "$ref" : "#/components/schemas/PersonTypeDefinisjon"
          },
          "navPoster" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/NavPostDefinisjon"
            }
          },
          "kommunePoster" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/KommunePostDefinisjon"
            }
          }
        }
      },
      "EkstraPostVerdiDefinisjon": {
        "required": [ "visningsnavn", "tekniskNavn" ],
        "type": "object",
        "properties": {
          "visningsnavn": {
            "type" : "string"
          },
          "tekniskNavn" : {
            "type": "string"
          }
        }
      },
      "PersonTypeDefinisjon": {
        "type" : "string",
        "enum" : [ "SOEKER", "EKTEFELLE", "PARTNER", "SAMBOER", "BARN", "SOESKEN", "MOR", "FAR", "MEDMOR", "ANNET" ]
      },
      "PostDefinisjon": {
        "required": [ "tekniskNavn", "visningsnavn", "beloep", "postType"],
        "type": "object",
        "properties": {
          "tekniskNavn" : {
            "type": "string"
          },
          "visningsnavn" : {
            "type": "string"
          },
          "beloep" : {
            "type" : "integer",
            "format" : "int64"
          },
          "postType": {
            "type" : "string",
            "enum" : [ "SKATTEETATEN", "NAV", "KOMMUNE" ]
          }
        }
      },
      "NavPostDefinisjon": {
        "required": [ "tekniskNavn", "beloep"],
        "type": "object",
        "properties": {
          "tekniskNavn" : {
            "type": "string"
          },
          "beloep" : {
            "type" : "integer",
            "format" : "int64"
          }
        }
      },
      "KommunePostDefinisjon": {
        "required": [ "visningsnavn", "beloep"],
        "type": "object",
        "properties": {
          "visningsnavn" : {
            "type": "string"
          },
          "beloep" : {
            "type" : "integer",
            "format" : "int64"
          }
        }
      },
      "OperasjonDefinisjon": {
        "type": "string",
        "enum": [ "ADDERE", "SUBTRAHERE", "INGEN"]
      },
      "BeregningstypeDefinisjon": {
        "type" : "string",
        "enum" : [ "PRAKTISK_BISTAND", "LANGTIDSOPPHOLD_INSTITUSJON", "BARNEHAGE_SFO", "BOLIGSOSIALE_FORMAAL" ]
      }
    }
  }
}
{
  "openapi": "3.0.3",
  "info": {
    "title": "Fiks register konfigurasjon service",
    "description": "Konfigurasjonstjeneste for Fiks register",
    "contact": {
      "name": "KS Fiks brukerstøtte",
      "url": "https://www.ks.no/fagomrader/digitalisering/utviklingsprosjekter",
      "email": "fiks-utvikling@ks.no"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://api.fiks.ks.no",
      "description": "KS Fiks API host",
      "variables": {}
    },
    {
      "url": "https://api.fiks.test.ks.no",
      "description": "KS Fiks TEST API host",
      "variables": {}
    }
  ],
  "tags": [
    {
      "name": "register-tjeneste-konfigurasjon",
      "description": "API for å håndtere konfigurasjon av Register tjenesten"
    }
  ],
  "paths": {
    "/register/api/v1/konfigurasjon/{fiksOrgId}": {
      "get": {
        "operationId": "hentRegisterTjenesteKonfigurasjon",
        "tags": [
          "register-tjeneste-konfigurasjon"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "fiksOrgId",
            "description": "",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Konfigurasjon for Register tjenesten",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegisterTjenesteDefinisjon"
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "opprettRegisterTjenesteKonfigurasjon",
        "tags": [
          "register-tjeneste-konfigurasjon"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "fiksOrgId",
            "description": "",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OpprettRegisterTjenesteRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Konfigurasjon for Register tjenesten er opprettet",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegisterTjenesteDefinisjon"
                }
              }
            },
            "links": {
              "self": {
                "operationRef": "hentRegisterTjenesteKonfigurasjon",
                "parameters": {
                  "fiksOrgId": "$request.path.fiksOrgId"
                }
              }
            }
          }
        }
      },
      "put": {
        "operationId": "oppdaterRegisterTjenesteKonfigurasjon",
        "tags": [
          "register-tjeneste-konfigurasjon"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "fiksOrgId",
            "description": "Id til organisasjonen som skal eie konfigurasjonen",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OppdaterRegisterTjenesteRequest"
              }
            }
          }
        },
        "responses": {
          "209": {
            "description": "Konfigurasjon ble opprettet",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegisterTjenesteDefinisjon"
                }
              }
            }
          },
          "403": {
            "description": "Mangler tilgang til å opprette tjenestekonfigurasjon for gitt organisasjon",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://developers.fiks.ks.no/api/fiks-common.json#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "fiksBearerToken": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "RegisterTjenesteDefinisjon": {
        "required": [
          "kontakteposter",
          "ressursId"
        ],
        "properties": {
          "ressursId": {
            "type": "string",
            "format": "uuid"
          },
          "kontakteposter": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "string"
            }
          }
        }
      },
      "OpprettRegisterTjenesteRequest": {
        "required": [
          "kontakteposter"
        ],
        "properties": {
          "kontakteposter": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "string"
            }
          }
        }
      },
      "OppdaterRegisterTjenesteRequest": {
        "required": [
          "kontakteposter"
        ],
        "properties": {
          "kontakteposter": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
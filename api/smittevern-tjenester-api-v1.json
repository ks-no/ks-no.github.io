  "tags" : [ {
    "name" : "SmittevernTjenesterPersonalia",
    "description" : "API for Smittevern tjenester"
  } ],
  "paths" : {
    "/smittevern-tjenester/api/v1/person/sokperson" : {
      "post" : {
        "tags" : [ "SmittevernTjenesterPersonalia" ],
        "description" : "Søker etter en person i Kontakt og reservasjonsregisteret og i Folkeregisteret",
        "operationId" : "hentPersonInfo",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/PersonSok"
              }
            }
          }
        },
        "responses" : {
          "401" : {
            "description" : "Ikke tilgang",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          },
          "200" : {
            "description" : "Kontaktopplysninger på person",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Person"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "timestamp" : {
            "type" : "integer",
            "format" : "int64"
          },
          "status" : {
            "type" : "integer",
            "format" : "int32"
          },
          "error" : {
            "type" : "string"
          },
          "errorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "path" : {
            "type" : "string"
          },
          "originalPath" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          },
          "errorCode" : {
            "type" : "string"
          },
          "errorJson" : {
            "type" : "string"
          }
        }
      },
      "Person" : {
        "type" : "object",
        "properties" : {
          "fornavn" : {
            "type" : "string"
          },
          "mellomnavn" : {
            "type" : "string"
          },
          "etternavn" : {
            "type" : "string"
          },
          "fodselsdato" : {
            "type" : "string"
          },
          "kjonn" : {
            "type" : "string",
            "enum" : [ "M", "K" ]
          },
          "epost" : {
            "type" : "string"
          },
          "telefonnummer" : {
            "type" : "string"
          },
          "adresse" : {
            "type" : "string"
          },
          "postnummer" : {
            "type" : "string"
          },
          "poststed" : {
            "type" : "string"
          },
          "statusKrr" : {
            "type" : "string",
            "enum" : [ "OK", "FAILED", "NOT_SUPPORTED", "NONE", "UNKNOWN" ]
          },
          "statusFolkeregister" : {
            "type" : "string",
            "enum" : [ "OK", "FAILED", "NOT_SUPPORTED", "NONE", "UNKNOWN" ]
          }
        }
      },
      "PersonSok" : {
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string"
          },
          "kommunenr" : {
            "pattern" : "^\\d{4}$",
            "type" : "string"
          }
        },
        "description" : "Info om søk"
      }
    }
  }
}
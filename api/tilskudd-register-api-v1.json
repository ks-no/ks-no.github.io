{
  "openapi": "3.1.1",
  "info": {
    "title": "KS Fiks: Tilskuddsregister",
    "description": "Service for Tilskudd",
    "contact": {
      "name": "Fiks-brukerstøtte",
      "url": "https://ks-no.github.io/",
      "email": "fiks-utvikling@ks.no"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://api.fiks.ks.no",
      "description": "KS Fiks API host"
    },
    {
      "url": "https://api.fiks.test.ks.no",
      "description": "KS Fiks TEST API host",
      "variables": {}
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "tags": [
    {
      "name": "RegisterOppslag",
      "description": "Åpne endepunkter for å slå opp i tilskuddsregisteret"
    },
    {
      "name": "RegisterEndring",
      "description": "Endepunkter for å gjøre endringer i tilskuddsregisteret"
    }
  ],
  "paths": {
    "/tilskuddsregister/api/v1/oppslag/ordning": {
      "get": {
        "tags": [
          "RegisterOppslag"
        ],
        "operationId": "soekEtterTilskuddsordning",
        "summary": "Lister ut tilgjengelig tilskuddsordninger",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SokEtterOrdningResponse"
                }
              }
            }
          }
        }
      }
    },
    "/tilskuddsregister/api/v1/oppslag/ordning/{id}": {
      "get": {
        "tags": [
          "RegisterOppslag"
        ],
        "operationId": "hentTilskuddsordning",
        "summary": "Henter ut en tilskuddsordning med id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "TilskuddsId",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OrdningDetaljer"
                }
              }
            }
          },
          "404": {
            "description": "Tilskudd ikke funnet",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/tilskuddsregister/api/v1/endring/ordning": {
      "post": {
        "tags": [
          "RegisterEndring"
        ],
        "operationId": "registrerTilskuddsordning",
        "summary": "Registrer en tilskuddsordning i tilskuddsregisteret",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegistrerNyOrdningRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Tilskudd registrert",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OrdningId"
                }
              }
            }
          },
          "default": {
            "description": "Noe gikk galt",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "ErrorMessage": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "error": {
            "type": "string"
          },
          "errorId": {
            "type": "string",
            "format": "uuid"
          },
          "path": {
            "type": "string"
          },
          "originalPath": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "errorCode": {
            "type": "string"
          },
          "errorJson": {
            "type": "string"
          }
        }
      },
      "OrdningId": {
        "type": "object",
        "properties": {
          "tilskuddsregisterId": {
            "type": "string",
            "format": "uuid",
            "readOnly": true
          },
          "tilskuddId": {
            "type": "string",
            "pattern": "^[A-Za-z0-9-]+$",
            "example": "TO-100001",
            "readOnly": true
          }
        }
      },
      "OrdningBase": {
        "type": "object",
        "properties": {
          "navn": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Navn"
            }
          },
          "etikett": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "status": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "formaal": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "stoerrelseMottaker": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "naering": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "regionsnivaa": {
            "$ref": "#/components/schemas/Regionsnivaa"
          },
          "region": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "varighet": {
            "$ref": "#/components/schemas/Varighet"
          },
          "ordningstype": {
            "$ref": "#/components/schemas/Ordningstype"
          },
          "rolle": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Rolle"
            }
          }
        }
      },
      "OrdningUtvidet": {
        "type": "object",
        "properties": {
          "stoettetiltaksnummer": {
            "type": "string",
            "description": "Id for støtteordning i Støtteregisteret"
          },
          "beskrivelse": {
            "type": "string"
          },
          "skjemaUrl": {
            "type": "string",
            "format": "uri"
          },
          "nettadresse": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri"
            }
          },
          "norskrettsligGrunnlag": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/NorskrettsligGrunnlag"
            }
          },
          "eoesrettsligGrunnlag": {
            "$ref": "#/components/schemas/EoesrettsligGrunnlag"
          },
          "aarligOekonomiskRamme": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AarligOekonomiskRamme"
            }
          }
        }
      },
      "OrdningDetaljer": {
        "type": "object",
        "readOnly": true,
        "required": [
          "tilskuddsregisterId",
          "tilskuddId",
          "opprettet",
          "endret",
          "navn",
          "etikett",
          "beskrivelse",
          "status",
          "type",
          "formaal",
          "stoerrelseMottaker",
          "naering",
          "regionsnivaa",
          "region",
          "varighet",
          "nettadresse",
          "ordningstype",
          "rolle",
          "norskrettsligGrunnlag",
          "aarligOekonomiskRamme"
        ],
        "allOf": [
          {
            "$ref": "#/components/schemas/OrdningId"
          },
          {
            "$ref": "#/components/schemas/OrdningBase"
          },
          {
            "$ref": "#/components/schemas/OrdningUtvidet"
          },
          {
            "type": "object",
            "properties": {
              "opprettet": {
                "type": "string",
                "format": "date-time",
                "example": "2025-08-27T15:45:30Z"
              },
              "endret": {
                "type": "string",
                "format": "date-time",
                "example": "2025-08-27T15:45:30Z"
              }
            }
          }
        ]
      },
      "OrdningElement": {
        "type": "object",
        "readOnly": true,
        "required": [
          "tilskuddsregisterId",
          "tilskuddId",
          "navn",
          "etikett",
          "status",
          "type",
          "formaal",
          "naering",
          "regionsnivaa",
          "region",
          "varighet",
          "ordningstype",
          "rolle"
        ],
        "allOf": [
          {
            "$ref": "#/components/schemas/OrdningId"
          },
          {
            "$ref": "#/components/schemas/OrdningBase"
          }
        ]
      },
      "SokEtterOrdningResponse": {
        "type": "object",
        "readOnly": true,
        "required": [
          "ordning"
        ],
        "properties": {
          "ordning": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrdningElement"
            }
          }
        }
      },
      "RegistrerNyOrdningRequest": {
        "type": "object",
        "writeOnly": true,
        "required": [
          "navn",
          "etikett",
          "status",
          "type",
          "formaal",
          "naering",
          "regionsnivaa",
          "region",
          "varighet",
          "ordningstype",
          "rolle",
          "beskrivelse",
          "norskrettsligGrunnlag",
          "aarligOekonomiskRamme"
        ],
        "allOf": [
          {
            "$ref": "#/components/schemas/OrdningBase"
          },
          {
            "$ref": "#/components/schemas/OrdningUtvidet"
          }
        ]
      },
      "Navn": {
        "type": "object",
        "required": [
          "tekst",
          "spraak"
        ],
        "properties": {
          "tekst": {
            "type": "string"
          },
          "spraak": {
            "type": "string",
            "enum": [
              "NOR",
              "ENG"
            ],
            "example": "NOR"
          }
        }
      },
      "Varighet": {
        "type": "object",
        "required": [
          "fraDato"
        ],
        "properties": {
          "fraDato": {
            "type": "string",
            "format": "date",
            "example": "2025-01-01"
          },
          "tildato": {
            "type": "string",
            "format": "date",
            "example": "2025-01-01"
          }
        }
      },
      "Rolleinnehaver": {
        "type": "object",
        "required": [
          "organisasjonsnummer"
        ],
        "properties": {
          "organisasjonsnummer": {
            "type": "string"
          },
          "navn": {
            "type": "string",
            "readOnly": true
          }
        }
      },
      "Rolle": {
        "type": "object",
        "required": [
          "type",
          "rolleinnehavertype",
          "rolleinnehaver"
        ],
        "properties": {
          "type": {
            "type": "string"
          },
          "rolleinnehavertype": {
            "type": "string",
            "enum": [
              "VIRKSOMHET"
            ],
            "example": "VIRKSOMHET"
          },
          "rolleinnehaver": {
            "$ref": "#/components/schemas/Rolleinnehaver"
          }
        }
      },
      "NorskrettsligGrunnlag": {
        "type": "object",
        "required": [
          "navn"
        ],
        "properties": {
          "navn": {
            "type": "string"
          },
          "nettadresse": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "EoesrettsligGrunnlag": {
        "type": "object",
        "required": [
          "primaerrettsligGrunnlag",
          "artikkel",
          "retningslinjeEsa"
        ],
        "properties": {
          "primaerrettsligGrunnlag": {
            "type": "string"
          },
          "artikkel": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "retningslinjeEsa": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "Beloep": {
        "type": "object",
        "required": [
          "beloep",
          "valuta"
        ],
        "properties": {
          "beloep": {
            "type": "number",
            "format": "double",
            "example": 53000
          },
          "valuta": {
            "type": "string",
            "example": "NOK",
            "description": "Valutakode i henhold til ISO 4217"
          }
        }
      },
      "AarligOekonomiskRamme": {
        "type": "object",
        "required": [
          "aar",
          "beloep"
        ],
        "properties": {
          "aar": {
            "type": "integer",
            "format": "int32",
            "example": 2025
          },
          "beloep": {
            "$ref": "#/components/schemas/Beloep"
          }
        }
      },
      "Regionsnivaa": {
        "type": "string",
        "enum": [
          "ALLE",
          "ALLE_UTENOM_SVALBARD",
          "SPESIFIKT_ANGITT"
        ],
        "example": "ALLE"
      },
      "Ordningstype": {
        "type": "string",
        "enum": [
          "ORDINAER",
          "AD_HOC"
        ],
        "example": "ORDINAER"
      }
    }
  }
}
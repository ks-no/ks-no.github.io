{
  "openapi": "3.0.1",
  "info": {
    "title": "KS Fiks: Varsling",
    "description": "Service for varsling med sms og epost",
    "contact": {
      "name": "Fiks-brukerstøtte",
      "url": "https://ks-no.github.io/",
      "email": "fiks-utvikling@ks.no"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://api.fiks.ks.no",
      "description": "KS Fiks API host"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "tags": [
    {
      "name": "varslingDeliveryReport",
      "description": "API for å håndtere funksjonalitet for leveringsrapporter "
    }
  ],
  "paths": {
    "/varsling/deliveryreport/make": {
      "post": {
        "tags": [
          "varslingDeliveryReport"
        ],
        "summary": "Mottar delivery report fra Make",
        "operationId": "receiveDeliveryReportMake",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DeliveryReportMake"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Motta delivery report",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "Noe gikk galt",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "ErrorMessage": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "error": {
            "type": "string"
          },
          "errorId": {
            "type": "string",
            "format": "uuid"
          },
          "path": {
            "type": "string"
          },
          "originalPath": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "errorCode": {
            "type": "string"
          },
          "errorJson": {
            "type": "string"
          }
        }
      },
      "DeliveryReportMake": {
        "required": [
          "messageId",
          "clientRef",
          "delivered",
          "statusCode"
        ],
        "type": "object",
        "properties": {
          "messageId": {
            "type": "string"
          },
          "clientRef": {
            "type": "string"
          },
          "delivered": {
            "type": "integer",
            "format": "int32"
          },
          "statusCode": {
            "type": "integer",
            "format": "int32"
          }
        }
      }
    }
  }
}
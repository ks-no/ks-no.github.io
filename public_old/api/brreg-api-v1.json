{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS FIKS: BRREG Service",
    "description" : "BRREG Proxy service",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://www.ks.no/fagomrader/digitalisering/utviklingsprosjekter/",
      "email" : "fiks@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.test.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "brreg",
    "description" : "API for henting av enheter fra enhetsregisteret i Brønnøysund"
  } ],
  "paths" : {
    "/brreg/api/brreg/enhet/{orgnr}" : {
      "get" : {
        "tags" : [ "brreg" ],
        "description" : "Henter orgnr",
        "operationId" : "hentEnhet",
        "parameters" : [ {
          "name" : "orgnr",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Organisasjonsenhet fra BRREG",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Enhet"
                }
              }
            }
          },
          "401" : {
            "description" : "Ingen enhet ble funnet for det oppgitte orgnr"
          }
        }
      }
    },
    "/brreg/api/brreg/enhet" : {
      "get" : {
        "tags" : [ "brreg" ],
        "description" : "Henter enheter",
        "operationId" : "hentEnheter",
        "parameters" : [ {
          "name" : "orgnr",
          "in" : "query",
          "required" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Enheter som samsvarer med orgnr",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Enhet"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Adresse" : {
        "type" : "object",
        "properties" : {
          "adresse" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          },
          "postnummer" : {
            "type" : "string"
          },
          "poststed" : {
            "type" : "string"
          },
          "land" : {
            "type" : "string"
          },
          "adresselinje1" : {
            "type" : "string"
          },
          "adresselinje2" : {
            "type" : "string"
          },
          "adresselinje3" : {
            "type" : "string"
          }
        }
      },
      "Enhet" : {
        "type" : "object",
        "properties" : {
          "organisasjonsnummer" : {
            "type" : "string"
          },
          "navn" : {
            "type" : "string"
          },
          "postadresse" : {
            "$ref" : "#/components/schemas/Adresse"
          },
          "forretningsadresse" : {
            "$ref" : "#/components/schemas/Adresse"
          }
        }
      }
    }
  }
}
{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: Innsyn Stats Service",
    "contact" : {
      "name" : "Fiks-brukerst√∏tte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.test.ks.no/"
  } ],
  "tags" : [ {
    "name" : "InnsynStats"
  } ],
  "paths" : {
    "/innsyn-stats/api/v1/stats" : {
      "get" : {
        "tags" : [ "InnsynStats" ],
        "operationId" : "getAllStats",
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/InnsynAlleStats"
                }
              }
            }
          }
        }
      }
    },
    "/innsyn-stats/api/v1/{fiksOrgId}/stats" : {
      "get" : {
        "tags" : [ "InnsynStats" ],
        "operationId" : "getStats",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        }, {
          "name" : "integrasjonId",
          "in" : "query",
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/InnsynOrganisasjonStats"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "InnsynAlleStats" : {
        "type" : "object",
        "properties" : {
          "stats" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/InnsynDocCountStat"
            }
          }
        },
        "description" : "Stats for alle organisasjoner"
      },
      "InnsynDocCountStat" : {
        "type" : "object",
        "properties" : {
          "fiksOrgId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "integrasjonId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "meldingstype" : {
            "type" : "string"
          },
          "docCount" : {
            "type" : "integer",
            "format" : "int64"
          }
        },
        "description" : "Stats for en kombinasjon av Fiks organisasjon id, integrasjon id og meldingstype"
      },
      "InnsynOrganisasjonStats" : {
        "type" : "object",
        "properties" : {
          "eierStats" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/InnsynDocCountStat"
            }
          }
        },
        "description" : "Stats for eide meldinger for en organisasjon"
      }
    }
  }
}
{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: MAtrikkel eier service",
    "description" : "KS Fiks: Matrikkel eier service",
    "contact" : {
      "name" : "Fiks brukerst√∏tte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "http://matrikkel-eier:8080",
    "description" : "Cluster internal service URI",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "MatrikkelEier",
    "description" : "API for Eier av matrikkel enhet"
  } ],
  "paths" : {
    "/matrikkel-eier/api/v1/sok" : {
      "post" : {
        "tags" : [ "MatrikkelEier" ],
        "description" : "Finn eiere for matrikkelenheter",
        "operationId" : "sok",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "uniqueItems" : true,
                "type" : "array",
                "items" : {
                  "$ref" : "#/components/schemas/MatrikkelnummerApiModel"
                }
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SokeresultatApiModel"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "EierApiModel" : {
        "type" : "object",
        "properties" : {
          "type" : {
            "type" : "string",
            "enum" : [ "JURIDISK_PERSON", "FYSISK_PERSON" ]
          },
          "verdi" : {
            "type" : "string"
          }
        }
      },
      "EnhetResultatApiModel" : {
        "type" : "object",
        "properties" : {
          "matrikkelnummer" : {
            "$ref" : "#/components/schemas/MatrikkelnummerApiModel"
          },
          "eiere" : {
            "uniqueItems" : true,
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/EierApiModel"
            }
          }
        }
      },
      "MatrikkelnummerApiModel" : {
        "required" : [ "bruksnummer", "festenummer", "gardsnummer", "kommunenummer", "seksjonsnummer" ],
        "type" : "object",
        "properties" : {
          "kommunenummer" : {
            "pattern" : "^\\d*$",
            "type" : "string"
          },
          "gardsnummer" : {
            "pattern" : "^\\d*$",
            "type" : "string"
          },
          "bruksnummer" : {
            "pattern" : "^\\d*$",
            "type" : "string"
          },
          "festenummer" : {
            "pattern" : "^\\d*$",
            "type" : "string"
          },
          "seksjonsnummer" : {
            "pattern" : "^\\d*$",
            "type" : "string"
          }
        }
      },
      "SokeresultatApiModel" : {
        "type" : "object",
        "properties" : {
          "enhetResultater" : {
            "uniqueItems" : true,
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/EnhetResultatApiModel"
            }
          }
        }
      }
    }
  }
}
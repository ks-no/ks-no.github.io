  "components" : {
    "schemas" : {
      "AvtaleDelegertTilSigneringDefinisjon" : {
        "required" : [ "fiksOrgId", "fnrSignatur", "tjenestekode" ],
        "type" : "object",
        "properties" : {
          "fiksOrgId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "tjenestekode" : {
            "pattern" : "^[0-9A-Z_.]+$",
            "type" : "string"
          },
          "fnrSignatur" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Fødselsnummer til personen som skal utføre signeringsoppdraget"
          }
        }
      },
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "timestamp" : {
            "type" : "integer",
            "format" : "int64"
          },
          "status" : {
            "type" : "integer",
            "format" : "int32"
          },
          "error" : {
            "type" : "string"
          },
          "errorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "path" : {
            "type" : "string"
          },
          "originalPath" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          },
          "errorCode" : {
            "type" : "string"
          },
          "errorJson" : {
            "type" : "string"
          }
        }
      },
      "DelegerSigneringAvAvtaleDefinisjon" : {
        "required" : [ "fnrSignatur", "tjenestekode" ],
        "type" : "object",
        "properties" : {
          "tjenestekode" : {
            "pattern" : "^[0-9A-Z_.]+$",
            "type" : "string"
          },
          "fnrSignatur" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Fødselsnummer til personen som skal utføre signeringsoppdraget"
          }
        }
      },
      "FiksOrganisasjonDefinisjon" : {
        "required" : [ "harHovedavtale", "id", "kommunenummer", "navn", "type" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string",
            "format" : "uuid"
          },
          "navn" : {
            "type" : "string"
          },
          "kommunenummer" : {
            "type" : "string"
          },
          "type" : {
            "type" : "string"
          },
          "harHovedavtale" : {
            "type" : "boolean"
          }
        }
      },
      "AvtaleSignertDefinisjon" : {
        "required" : [ "avtaleUuid", "fiksOrgId", "fiksOrgNavn", "gjeldendeVersjon", "signeringStatus", "signertVersjon", "tjenestekode", "tjenestenavn", "uuid" ],
        "type" : "object",
        "properties" : {
          "uuid" : {
            "type" : "string",
            "format" : "uuid"

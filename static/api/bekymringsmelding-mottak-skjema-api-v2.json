          "kommunenummer" : {
            "pattern" : "^\\d{4}$",
            "type" : "string",
            "description" : "Kommunenummer hvor bekymringsmeldingen skal sendes til"
          },
          "kommunenavn" : {
            "minLength" : 1,
            "type" : "string",
            "description" : "Kommunenavn hvor bekymringsmeldingen skal sendes til"
          },
          "bydelsnummer" : {
            "pattern" : "^\\d{2}$",
            "type" : "string",
            "description" : "Bydelsnummeret bekymringsmeldingen skal sendes til"

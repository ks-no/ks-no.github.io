      }
    },
    "schemas": {
      "ErrorMessage": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "error": {
            "type": "string"
          },
          "errorId": {
            "type": "string",
            "format": "uuid"
          },
          "path": {
            "type": "string"
          },
          "originalPath": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "errorCode": {
            "type": "string"
          },
          "errorJson": {
            "type": "string"
          }
        }
      },
      "AdministratorerResponse": {
        "required": [
          "administratorer"
        ],
        "type": "object",
        "properties": {
          "administratorer": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AdminResponse"
            }
          }
        },
        "description": "Administratorer"
      },
      "PersonerResponse": {
        "required": [
          "brukere"
        ],
        "type": "object",
        "properties": {
          "brukere": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PersonResponse"
            }
          }
        },
        "description": "Personer"
      },

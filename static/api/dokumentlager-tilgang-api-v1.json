        "required": [
          "personId",
          "navn"
        ],
        "type": "object",
        "properties": {
          "navn": {
            "type": "string"
          },
          "personId": {
            "description": "Personens unike Id",
            "type": "string",
            "format": "uuid"
          }
        },
        "description": "Admin"
      },
      "PersonResponse": {
        "required": [
          "personId",
          "navn",
          "tilganger"

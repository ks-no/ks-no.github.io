{
  "openapi": "3.0.1",
  "info": {
    "title": "Tulle-API",
    "version": "1.0",
    "license": {
      "name": "MIT"
    }
  },
  "servers": [
    {
      "url": "https://api.example.com"
    }
  ],
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "paths": {
    "/svar": {
      "get": {
        "summary": "Gir deg svaret",
        "operationId": "getSvar",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Svaret er 42",
            "content": {
              "application/json": {
                "example": {
                  "svar": 42
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key"
      }
    }
  }
}
  "tags" : [ {
    "name" : "katjingProdukter",
    "description" : "API for henting og oppdatering av produkter i katjing."
  } ],
  "paths" : {
    "/katjing/api/v1/produkter" : {
      "get" : {
        "tags" : [ "katjingProdukter" ],
        "description" : "Henter alle registrerte produkter",
        "operationId" : "getProdukter",
        "responses" : {
          "200" : {
            "description" : "Registrete produkter",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "type" : "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/katjing/api/v1/produkter/{produktKode}/priser" : {
      "post" : {
        "tags" : [ "katjingProdukter" ],
        "description" : "oppretter en ny pris for et produkt fra en gitt dato",
        "operationId" : "opprettNyPris",
        "parameters" : [ {
          "name" : "produktKode",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "enum" : [ "DIGISOS", "FIKSIO", "DOKUMENTLAGER", "INNSYN", "KJORETOYREGISTER", "DHIS2", "DHIS2_FASTPRIS", "BEKYMRINGSMELDING_SVARINN", "BEKYMRINGSMELDING_BYDEL", "PROVESVAR", "SVARUT_ALLE", "SVARUT_ALLE_STATLIGE", "SVARUT_ALTINN", "SVARUT_SVARINN", "SVARUT_SMS_INNLAND", "SVARUT_SMS_UTLAND", "FOLKEREGISTER", "SVARUT_GAMLE_DIGISOS", "MINSIDE_FASTPRIS", "DELDOKUMENT_FASTPRIS", "VAKSINE", "KLINIKERMELDING", "INNREISE", "INNREISE_OPPFOLGING", "VAKSINESTATUS", "DIGIORDEN_FASTPRIS", "DIGIORDEN_FASTPRIS_MELLOM", "DIGIORDEN_FASTPRIS_STOR", "VARSLING_NORGE", "VARSLING_UTLAND", "KSLARING_SSO" ]
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/OpprettPrisRequest"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : { }
            }
          }
        }
      },
      "delete" : {
        "tags" : [ "katjingProdukter" ],
        "description" : "sletter alle priser for spesifisert produkt",
        "operationId" : "slettAlle",
        "parameters" : [ {
          "name" : "produktKode",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "enum" : [ "DIGISOS", "FIKSIO", "DOKUMENTLAGER", "INNSYN", "KJORETOYREGISTER", "DHIS2", "DHIS2_FASTPRIS", "BEKYMRINGSMELDING_SVARINN", "BEKYMRINGSMELDING_BYDEL", "PROVESVAR", "SVARUT_ALLE", "SVARUT_ALLE_STATLIGE", "SVARUT_ALTINN", "SVARUT_SVARINN", "SVARUT_SMS_INNLAND", "SVARUT_SMS_UTLAND", "FOLKEREGISTER", "SVARUT_GAMLE_DIGISOS", "MINSIDE_FASTPRIS", "DELDOKUMENT_FASTPRIS", "VAKSINE", "KLINIKERMELDING", "INNREISE", "INNREISE_OPPFOLGING", "VAKSINESTATUS", "DIGIORDEN_FASTPRIS", "DIGIORDEN_FASTPRIS_MELLOM", "DIGIORDEN_FASTPRIS_STOR", "VARSLING_NORGE", "VARSLING_UTLAND", "KSLARING_SSO" ]
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : { }
            }
          }
        }
      }
    },
    "/katjing/api/v1/produkter/{produktKode}/priser/siste" : {
      "delete" : {
        "tags" : [ "katjingProdukter" ],
        "description" : "sletter gjeldende pris for spesifisert produkt",
        "operationId" : "slettSistePris",
        "parameters" : [ {
          "name" : "produktKode",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "enum" : [ "DIGISOS", "FIKSIO", "DOKUMENTLAGER", "INNSYN", "KJORETOYREGISTER", "DHIS2", "DHIS2_FASTPRIS", "BEKYMRINGSMELDING_SVARINN", "BEKYMRINGSMELDING_BYDEL", "PROVESVAR", "SVARUT_ALLE", "SVARUT_ALLE_STATLIGE", "SVARUT_ALTINN", "SVARUT_SVARINN", "SVARUT_SMS_INNLAND", "SVARUT_SMS_UTLAND", "FOLKEREGISTER", "SVARUT_GAMLE_DIGISOS", "MINSIDE_FASTPRIS", "DELDOKUMENT_FASTPRIS", "VAKSINE", "KLINIKERMELDING", "INNREISE", "INNREISE_OPPFOLGING", "VAKSINESTATUS", "DIGIORDEN_FASTPRIS", "DIGIORDEN_FASTPRIS_MELLOM", "DIGIORDEN_FASTPRIS_STOR", "VARSLING_NORGE", "VARSLING_UTLAND", "KSLARING_SSO" ]
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : { }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "OpprettPrisRequest" : {
        "required" : [ "fra", "pris" ],
        "type" : "object",
        "properties" : {
          "fra" : {
            "$ref" : "#/components/schemas/YearMonthApiModel"
          },
          "pris" : {
            "type" : "number",
            "format" : "double"
          }
        }
      },
      "YearMonthApiModel" : {
        "required" : [ "month", "year" ],
        "type" : "object",
        "properties" : {
          "year" : {
            "minimum" : 1970,
            "type" : "integer",
            "format" : "int32"
          },
          "month" : {
            "maximum" : 12,
            "minimum" : 1,
            "type" : "integer",
            "format" : "int32"
          }
        }
      }
    }
  }
}
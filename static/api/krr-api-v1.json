{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS FIKS: KRR Service",
    "description" : "KRR service",
    "contact" : {
      "name" : "Fiks-brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "krr",
    "description" : "API for henting av data fra kontakt- og reservasjonsregisteret hos Digdir"
  } ],
  "paths" : {
    "/krr/api/krr/person" : {
      "post" : {
        "tags" : [ "Kontakt- og reservasjonsregisteret" ],
        "description" : "Henter kontaktinfo for et personnr",
        "operationId" : "kontaktInfoPerson",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/SokRequest"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "200" : {
            "description" : "Kontaktinfo for et personnr",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Person"
                }
              }
            }
          }
        }
      }
    },
    "/krr/api/krr/me" : {
      "get" : {
        "tags" : [ "Kontakt- og reservasjonsregisteret" ],
        "description" : "Henter informasjon for autentisert bruker",
        "operationId" : "me",
        "responses" : {
          "200" : {
            "description" : "Personinformasjon for autentisert bruker",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Person"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Epost" : {
        "type" : "object",
        "properties" : {
          "epost" : {
            "type" : "string"
          },
          "sistOppdatert" : {
            "type" : "string",
            "format" : "date-time"
          },
          "sistVerifisert" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "Person" : {
        "type" : "object",
        "properties" : {
          "fnr" : {
            "type" : "string"
          },
          "reservert" : {
            "type" : "boolean"
          },
          "status" : {
            "type" : "string",
            "enum" : [ "AKTIV", "SLETTET", "IKKE_REGISTRERT" ]
          },
          "kanVarsles" : {
            "type" : "boolean"
          },
          "tlf" : {
            "$ref" : "#/components/schemas/Tlf"
          },
          "epost" : {
            "$ref" : "#/components/schemas/Epost"
          },
          "digitalpostadresse" : {
            "type" : "string"
          },
          "digitalpostkasseleverandoerAdresse" : {
            "type" : "string"
          }
        }
      },
      "Tlf" : {
        "type" : "object",
        "properties" : {
          "tlf" : {
            "type" : "string"
          },
          "sistOppdatert" : {
            "type" : "string",
            "format" : "date-time"
          },
          "sistVerifisert" : {
            "type" : "string",
            "format" : "date-time"
          }
        }
      },
      "SokRequest" : {
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string"
          }
        },
        "description" : "En forespørsel om å hente personopplysninger"
      }
    }
  }
}
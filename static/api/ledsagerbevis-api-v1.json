{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: Ledsagerbevis",
    "description" : "Tjeneste digitalt ledsagerbevis",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  }, {
    "url" : "https://api.test.fiks.ks.no",
    "description" : "KS Fiks TEST API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "Ledsagerbevis",
    "description" : "API for Digitalt ledsagerbevis: Oppslag mot Folkeregister"
  } ],
  "paths" : {
    "/ledsagerbevis/api/v1/folkeregister/organisasjoner/{fiksOrgId}" : {
      "post" : {
        "tags" : [ "Ledsagerbevis" ],
        "description" : "Oppslag med fødselsnummer for å hente ut informasjon fra Folkeregisteret",
        "operationId" : "oppslag",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/FolkeregisterRequest"
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Informasjon henta fra Folkeregisteret",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/FolkeregisterPerson"
                }
              }
            }
          }
        }
      }
    },
    "/ledsagerbevis/api/v1" : {
      "get" : {
        "tags" : [ "Ledsagerbevis" ],
        "description" : "Test",
        "operationId" : "test",
        "responses" : {
          "200" : {
            "description" : "Informasjon om tjeneste",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Test"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Adresse" : {
        "type" : "object",
        "properties" : {
          "adressenavn" : {
            "type" : "string",
            "description" : "Gatenavn"
          },
          "postnummer" : {
            "pattern" : "^[0-9]{4}$",
            "type" : "string",
            "description" : "Postnummer"
          },
          "poststed" : {
            "type" : "string",
            "description" : "Poststed"
          },
          "kommunenummer" : {
            "pattern" : "^\\d{4}$",
            "type" : "string",
            "description" : "Kommunenummer"
          }
        },
        "description" : "Adresse det er mulig å sende post til"
      },
      "FolkeregisterPerson" : {
        "required" : [ "fnr" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "type" : "string"
          },
          "navn" : {
            "type" : "string",
            "description" : "Navn henta fra Folkeregisteret."
          },
          "adresse" : {
            "$ref" : "#/components/schemas/Adresse"
          }
        },
        "description" : "Personinformasjon henta fra Folkeregisteret"
      },
      "FolkeregisterRequest" : {
        "required" : [ "fnr" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Fødselsnummeret det skal hentes informasjon om fra Folkeregisteret"
          }
        },
        "description" : "Request for å hente ut informasjon fra Folkeregisteret"
      },
      "Test" : {
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}
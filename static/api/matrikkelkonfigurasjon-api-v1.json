{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS FIKS: matrikkelurl Service",
    "description" : "Matrikkelurl service",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://www.ks.no/fagomrader/digitalisering/utviklingsprosjekter/",
      "email" : "fiks@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "matrikkelurler",
    "description" : "API for urler på matrikkelenheter"
  } ],
  "paths" : {
    "/matrikkelkonfigurasjon/api/v1/matrikkelurler/urler/{id}" : {
      "put" : {
        "tags" : [ "matrikkelurler" ],
        "description" : "Oppdater url",
        "operationId" : "oppdaterUrl",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/OppdatertUrl"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "204" : {
            "description" : "Url lagret"
          }
        }
      },
      "delete" : {
        "tags" : [ "matrikkelurler" ],
        "description" : "Sletter url",
        "operationId" : "SlettUrl",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "204" : {
            "description" : "Url slettet",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Url"
                }
              }
            }
          }
        }
      }
    },
    "/matrikkelkonfigurasjon/api/v1/matrikkelurler/urler/{kommunenr}" : {
      "get" : {
        "tags" : [ "matrikkelurler" ],
        "description" : "Henter kommune urler",
        "operationId" : "urlerByKommunenr",
        "parameters" : [ {
          "name" : "kommunenr",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Urler til matrikkelenheter i kommunen",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Url"
                  }
                }
              }
            }
          },
          "404" : {
            "description" : "Ingen urler ble funnet for det oppgitte kommunenr"
          }
        }
      }
    },
    "/matrikkelkonfigurasjon/api/v1/matrikkelurler/urler/fiks/{fiksOrg}" : {
      "get" : {
        "tags" : [ "matrikkelurler" ],
        "description" : "Henter kommune urler",
        "operationId" : "urlerByOrgid",
        "parameters" : [ {
          "name" : "fiksOrg",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Urler til matrikkelenheter i kommunen",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Url"
                  }
                }
              }
            }
          },
          "404" : {
            "description" : "Ingen urler ble funnet for det oppgitte kommunenr"
          }
        }
      }
    },
    "/matrikkelkonfigurasjon/api/v1/matrikkelurler/urler" : {
      "post" : {
        "tags" : [ "matrikkelurler" ],
        "description" : "Lagre ny url",
        "operationId" : "nyUrl",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/NyUrl"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "200" : {
            "description" : "Url lagret",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "string",
                  "format" : "uuid"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "OppdatertUrl" : {
        "required" : [ "url", "urltekst" ],
        "type" : "object",
        "properties" : {
          "url" : {
            "pattern" : "https?:\\/\\/.*",
            "type" : "string"
          },
          "urltekst" : {
            "type" : "string"
          }
        }
      },
      "Url" : {
        "type" : "object",
        "properties" : {
          "url" : {
            "type" : "string"
          },
          "urltekst" : {
            "type" : "string"
          },
          "fiksorg" : {
            "type" : "string",
            "format" : "uuid"
          },
          "id" : {
            "type" : "string",
            "format" : "uuid"
          }
        }
      },
      "NyUrl" : {
        "required" : [ "fiksorg", "url", "urltekst" ],
        "type" : "object",
        "properties" : {
          "url" : {
            "pattern" : "https?:\\/\\/.*",
            "type" : "string"
          },
          "fiksorg" : {
            "type" : "string",
            "format" : "uuid"
          },
          "urltekst" : {
            "type" : "string"
          }
        }
      }
    }
  }
}
  "paths" : {
    "/innsyn-sok/api/v1/oppslag/dokument" : {
      "get" : {
        "tags" : [ "Oppslag" ],
        "summary" : "Hent en melding basert på dokumentId",
        "operationId" : "lookupDokument",
        "parameters" : [ {
          "name" : "onBehalfOf",
          "in" : "query",
          "description" : "Organisasjonsnummer for organisasjonen dette søket skal utføres på vegne av. Krever at autentisert bruker har rettigheter til å agere på vegne av denne organisasjonen.",
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "dokumentId",
          "in" : "query",
          "description" : "Id for dokumentet (formatert som <fiksOrgId>_<integrasjonId>_<meldingType>_<meldingId>)",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "akseptertMeldingVersjon",
          "in" : "query",
          "description" : "Akseptert melding-versjon. Kan settes flere ganger. Innsyn vil nedgradere treff på høyere versjon til den høyeste av de aksepterte versjonen (om det er oppgitt flere på samme dokumenttype)",
          "schema" : {
            "uniqueItems" : true,
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/MeldingVersjon"
            }
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/OppslagResultat"
                }
              }
            }
          }
        }
      }
    },
    "/innsyn-sok/api/v1/oppslag/forelder" : {
      "get" : {
        "tags" : [ "Oppslag" ],
        "summary" : "Hent en forelder-melding og dennes barn-meldinger",
        "operationId" : "lookupForelder",
        "parameters" : [ {
          "name" : "onBehalfOf",
          "in" : "query",
          "description" : "Organisasjonsnummer for organisasjonen dette søket skal utføres på vegne av. Krever at autentisert bruker har rettigheter til å agere på vegne av denne organisasjonen.",
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "forelderId",
          "in" : "query",
          "description" : "Id for forelder-dokumentet",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "forelderType",
          "in" : "query",
          "description" : "Typen til forelder-dokumentet",
          "required" : true,
          "schema" : {
            "$ref" : "#/components/schemas/MeldingType"
          }
        }, {
          "name" : "akseptertMeldingVersjon",
          "in" : "query",
          "description" : "Akseptert melding-versjon. Kan settes flere ganger. Innsyn vil nedgradere treff på høyere versjon til den høyeste av de aksepterte versjonen (om det er oppgitt flere på samme dokumenttype)",
          "schema" : {
            "uniqueItems" : true,
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/MeldingVersjon"
            }
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ForelderOppslagResultat"
                }
              }
            }
          }
        }
      }
    },
    "/innsyn-sok/api/v1/oppslag/korrelasjon" : {
      "get" : {
        "tags" : [ "Oppslag" ],
        "summary" : "Hent alle meldinger som er korrelert på den spesifiserte meldingsid'en",
        "operationId" : "lookupKorrelasjon",
        "parameters" : [ {
          "name" : "onBehalfOf",
          "in" : "query",
          "description" : "Organisasjonsnummer for organisasjonen dette søket skal utføres på vegne av. Krever at autentisert bruker har rettigheter til å agere på vegne av denne organisasjonen.",
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "korrelasjonId",
          "in" : "query",
          "description" : "Korrelasjon-id som marker dokumentene som relatert",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "akseptertMeldingVersjon",
          "in" : "query",
          "description" : "Akseptert melding-versjon. Kan settes flere ganger. Innsyn vil nedgradere treff på høyere versjon til den høyeste av de aksepterte versjonen (om det er oppgitt flere på samme dokumenttype)",
          "schema" : {
            "uniqueItems" : true,
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/MeldingVersjon"
            }
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/KorrelasjonOppslag"
                }
              }
            }
          }
        }
      }
    }
  },

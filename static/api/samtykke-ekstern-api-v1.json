      "put" : {
        "tags" : [ "samtykke-ekstern" ],
        "description" : "Oppdaterer samtykke-status på vegne av part",
        "operationId" : "oppdaterSamtykkeStatusForPart",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        }, {
          "name" : "samtykkeId",
          "in" : "path",

        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/EndreInnbyggertjenesterTilgangForPartRequest"
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Endret tilgang til innbyggertjenester for part"
          },
          "default" : {

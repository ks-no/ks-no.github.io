{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: Samtykke",
    "description" : "Fiks Samtykke Ekstern service",
    "contact" : {
      "name" : "Fiks-brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host"
  }, {
    "url" : "https://api.fiks.test.ks.no",
    "description" : "KS Fiks API test host"
  } ],
  "tags" : [ {
    "name" : "samtykke-ekstern",
    "description" : "API for å opprette og slette nytt samtykke"
  } ],
  "paths" : {
    "/fiks-samtykke/api/v1/ekstern/{fiksOrgId}/opprettsamtykke" : {
      "post" : {
        "tags" : [ "samtykke-ekstern" ],
        "description" : "Oppretter nytt samtykke",
        "operationId" : "opprettSamtykke",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/OpprettSamtykkeRequest"
              }
            }
          }
        },
        "responses" : {
          "201" : {
            "description" : "Opprettet nytt samtykke"
          },
          "default" : {
            "description" : "Noe gikk galt",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    },
    "/fiks-samtykke/api/v1/ekstern/{fiksOrgId}/slettsamtykke" : {
      "delete" : {
        "tags" : [ "samtykke-ekstern" ],
        "description" : "Oppretter nytt samtykke",
        "operationId" : "slettSamtykke",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/SlettSamtykkeRequest"
              }
            }
          }
        },
        "responses" : {
          "201" : {
            "description" : "Slettet samtykke"
          },
          "default" : {
            "description" : "Noe gikk galt.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorMessage"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "ErrorMessage" : {
        "type" : "object",
        "properties" : {
          "timestamp" : {
            "type" : "integer",
            "format" : "int64"
          },
          "status" : {
            "type" : "integer",
            "format" : "int32"
          },
          "error" : {
            "type" : "string"
          },
          "errorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "path" : {
            "type" : "string"
          },
          "originalPath" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          },
          "errorCode" : {
            "type" : "string"
          },
          "errorJson" : {
            "type" : "string"
          }
        }
      },
      "MultipartFile" : {
        "type" : "object",
        "properties" : {
          "originalFilename" : {
            "type" : "string"
          },
          "name" : {
            "type" : "string"
          },
          "bytes" : {
            "type" : "array",
            "items" : {
              "type" : "string",
              "format" : "byte"
            }
          },
          "empty" : {
            "type" : "boolean"
          },
          "resource" : {
            "$ref" : "#/components/schemas/Resource"
          },
          "size" : {
            "type" : "integer",
            "format" : "int64"
          },
          "inputStream" : {
            "type" : "object"
          },
          "contentType" : {
            "type" : "string"
          }
        },
        "description" : "Dokument som beskriver samtykket"
      },
      "OpprettSamtykkeRequest" : {
        "required" : [ "akseptfrist", "dokument", "gyldigTil", "mappeId", "personer", "samtykkeId" ],
        "type" : "object",
        "properties" : {
          "personer" : {
            "$ref" : "#/components/schemas/Personer"
          },
          "mappeId" : {
            "type" : "string",
            "description" : "Mappe-id knyttet til samtykket"
          },
          "samtykkeId" : {
            "type" : "string",
            "description" : "Id knyttet til samtykket"
          },
          "dokument" : {
            "$ref" : "#/components/schemas/MultipartFile"
          },
          "akseptfrist" : {
            "type" : "string",
            "description" : "Dato som forteller signeringsfrist for samtykke",
            "format" : "date"
          },
          "gyldigTil" : {
            "type" : "string",
            "description" : "Dato som forteller hvor lenge samtykket vil være gyldig",
            "format" : "date"
          }
        }
      },
      "Person" : {
        "required" : [ "fnr", "navn" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string",
            "description" : "Personnummer for person"
          },
          "navn" : {
            "type" : "string",
            "description" : "Navn for person"
          }
        }
      },
      "Personer" : {
        "type" : "object",
        "properties" : {
          "personer" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Person"
            }
          }
        },
        "description" : "Personer som skal gi samtykke for sak"
      },
      "Resource" : {
        "type" : "object",
        "properties" : {
          "contentAsByteArray" : {
            "type" : "array",
            "items" : {
              "type" : "string",
              "format" : "byte"
            }
          },
          "filename" : {
            "type" : "string"
          },
          "open" : {
            "type" : "boolean"
          },
          "file" : {
            "type" : "string",
            "format" : "binary"
          },
          "readable" : {
            "type" : "boolean"
          },
          "url" : {
            "type" : "string",
            "format" : "url"
          },
          "description" : {
            "type" : "string"
          },
          "uri" : {
            "type" : "string",
            "format" : "uri"
          },
          "inputStream" : {
            "type" : "object"
          }
        }
      },
      "SlettSamtykkeRequest" : {
        "required" : [ "samtykkeId" ],
        "type" : "object",
        "properties" : {
          "samtykkeId" : {
            "type" : "string",
            "description" : "Saksnummer knyttet til samtykket som skal slettes"
          }
        }
      }
    }
  }
}
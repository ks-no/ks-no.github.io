        "properties" : {
          "navn" : {
            "minLength" : 1,
            "type" : "string",
            "description" : "Navn for person"
          },
          "samtykkeSvarStatus" : {
            "minLength" : 1,
            "type" : "string",
            "description" : "Status for samtykke",

          },
          "samtykkerAktiv" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Samtykke"
            }
          },
          "samtykkerUtgaatt" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Samtykke"

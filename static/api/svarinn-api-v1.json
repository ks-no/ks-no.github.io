          "filmetadata" : {
            "type" : "array",
            "description" : "Metadata for filene i forsendelsen",
            "items" : {
              "$ref" : "#/components/schemas/FilMetadata"
            }
          },
          "svarSendesTil" : {
            "$ref" : "#/components/schemas/SvarSendesTil"
          },
          "svarPaForsendelse" : {
            "type" : "string",
            "description" : "Id for forsendelse som denne forsendelsen er et svar på. UUID.",

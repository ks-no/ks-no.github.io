            "example" : "Storevik"
          },
          "navn" : {
            "type" : "string",
            "description" : "Navn",
            "example" : "Ola Nordmann"
          },
          "land" : {
            "type" : "string",
            "description" : "Land",
            "example" : "Norge"
          },
          "orgnr" : {
            "type" : "string",
            "description" : "Organisasjonsnummer",
            "example" : "123456789"
          }
        },
        "description" : "Mottaker av forsendelsen"
      },
      "NoarkMetadataForImport" : {
        "type" : "object",
        "properties" : {
          "sakssekvensnummer" : {
            "type" : "integer",
